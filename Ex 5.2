import numpy as np
arr = np.arange(1,13)
print("Original 1D Array:",arr)
reshaped_arr_2d = arr.reshape(3,4)
print("Reshaped 2D Array(3x4):\n",reshaped_arr_2d)
reshaped_arr_3d = arr.reshape(2,3,2)
print("Reshaped 3D Array(2x3x2):\n",reshaped_arr_3d)
flattened_arr = reshaped_arr_3d.flatten()
print("Flattened Array:",flattened_arr)




